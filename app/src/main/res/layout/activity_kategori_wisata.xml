                <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/judul_gunung"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="24dp"
                        android:layout_marginLeft="24dp"
                        android:layout_marginBottom="24dp"
                        android:fontFamily="@font/mmedium"
                        android:text="Gunung"
                        android:textColor="#ffffff"
                        android:textSize="18dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="24dp"
                        android:layout_marginLeft="24dp"
                        android:layout_marginBottom="8dp"
                        android:fontFamily="@font/mregular"
                        android:text="Kunjungi"
                        android:textColor="#ffffff"
                        android:textSize="10sp"
                        app:layout_constraintBottom_toTopOf="@+id/judul_gunung"
                        app:layout_constraintStart_toStartOf="parent" />
                </android.support.constraint.ConstraintLayout>

            </com.github.florent37.shapeofview.shapes.RoundRectView>

            <com.github.florent37.shapeofview.shapes.RoundRectView
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_marginLeft="24dp"
                android:layout_marginRight="24dp"
                android:elevation="4dp"
                app:shape_roundRect_bottomLeftRadius="4dp"
                app:shape_roundRect_bottomRightRadius="4dp"
                app:shape_roundRect_topLeftRadius="4dp"
                app:shape_roundRect_topRightRadius="4dp"
                android:layout_marginBottom="16dp"
                >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/taman"
                    android:scaleType="centerCrop"/>

                <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/judul_taman"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="24dp"
                        android:layout_marginLeft="24dp"
                        android:layout_marginBottom="24dp"
                        android:fontFamily="@font/mmedium"
                        android:text="Taman"
                        android:textColor="#ffffff"
                        android:textSize="18dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="24dp"
                        android:layout_marginLeft="24dp"
                        android:layout_marginBottom="8dp"
                        android:fontFamily="@font/mregular"
                        android:text="Kunjungi"
                        android:textColor="#ffffff"
                        android:textSize="10sp"
                        app:layout_constraintBottom_toTopOf="@+id/judul_taman"
                        app:layout_constraintStart_toStartOf="parent" />
                </android.support.constraint.ConstraintLayout>

            </com.github.florent37.shapeofview.shapes.RoundRectView>









        </LinearLayout>


